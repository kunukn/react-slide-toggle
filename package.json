{
  "name": "react-slide-toggle",
  "version": "0.1.13",
  "dependencies": {},
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack-dev-server --config webpack.config.js --mode development",
    "build": "webpack --config webpack.config.build.js --mode production",
    "minify": "npm run minify-umd",
    "minify-all": "npm run minify-umd & npm run minify-var",
    "minify-umd": "uglifyjs dist/ReactSlideToggle.umd.js --config-file uglify.config.json --output dist/ReactSlideToggle.umd.min.js",
    "minify-var": "uglifyjs dist/ReactSlideToggle.var.js --config-file uglify.config.json --output dist/ReactSlideToggle.var.min.js",
    "deploy": "npm run build && npm run minify"
  },
  "description": "React component version of jQuery.slideToggle",
  "main": "dist/ReactSlideToggle.umd.min.js",
  "umd": "dist/ReactSlideToggle.umd.js",
  "unpkg": "dist/ReactSlideToggle.umd.min.js",
  "author": "kunukn",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "browserslist": "last 2 versions, ie 11",
  "babel": {
    "presets": [
      "env",
      "stage-2",
      "react"
    ]
  },
  "peerDependencies": {
    "react": "^15.x || ^16.x",
    "react-dom": "^15.x || ^16.x"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.1",
    "babel-core": "6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "bezier-easing": "^2.1.0",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "5.1.6",
    "css-loader": "0.28.11",
    "eases": "^1.0.8",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "hoek": "^4.2.1",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "4.9.0",
    "parallel-webpack": "^2.3.0",
    "postcss-loader": "^2.1.5",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "webpack": "4.12.0",
    "webpack-cli": "^3.0.3",
    "webpack-dev-server": "3.1.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kunukn/react-slide-toggle.git"
  },
  "keywords": [
    "slide",
    "toggle",
    "collapsible"
  ],
  "bugs": {
    "url": "https://github.com/kunukn/react-slide-toggle/issues"
  },
  "homepage": "https://github.com/kunukn/react-slide-toggle#readme"
